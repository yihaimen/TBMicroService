{
  "name": "TBMicroService",
  "version": "1.0.0",
  "description": "Connect different frame.",
  "private": true,
  "scripts": {
    "dev": "webpack serve --open --progress --config webpack.config.js",
    "build": "webpack --progress --env production --config webpack.config.js",
    "local": "http-server dist",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,mjs,ts,tsx}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/*.{css,scss,less,json,html,md,markdown}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yihaimen/TBMicroService.git"
  },
  "keywords": [
    "MicroService",
    "ThoughtBytes",
    "React",
    "TypeScript",
    "Webpack"
  ],
  "author": {
    "name": "ThoughtBytes",
    "email": "yihaimen@outlook.com",
    "url": "https://yihaimen.github.io/"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yihaimen/TBMicroService/issues"
  },
  "homepage": "https://github.com/yihaimen/TBMicroService#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/autoprefixer": "^9.7.2",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.5",
    "less": "^3.13.0",
    "less-loader": "^7.1.0",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "dependencies": {
    "antd": "^4.9.2",
    "autoprefixer": "^10.1.0",
    "postcss-easy-import": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%"
  ]
}
